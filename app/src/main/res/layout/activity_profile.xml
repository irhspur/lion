<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="phonedirectory.lion.com.lion.Profile"
    >

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/ScrollView01" android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        >


    <RelativeLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:padding="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Name not available"
            android:id="@+id/name"
            android:padding="5dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/name_profile"
            android:id="@+id/name_title"
            android:padding="5dp"
            android:layout_below="@id/name"
            />

        <View style="@style/Divider"
            android:layout_marginBottom="5dp"
            android:layout_below="@id/name_title"
            android:id="@+id/divider_contact"/>
<!--...............................................................................-->

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Address not available"
            android:id="@+id/address"
            android:padding="5dp"
            android:layout_below="@id/divider_contact"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/address_title"
            android:id="@+id/address_title"
            android:padding="5dp"
            android:layout_below="@id/address"/>

        <View style="@style/Divider"
            android:layout_marginBottom="5dp"
            android:layout_below="@id/address_title"
            android:id="@+id/divider_contact2"/>

<!--....................................................................-->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Phone number not available"
        android:id="@+id/phone"
        android:padding="5dp"
        android:layout_below="@id/divider_contact2"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/phone_title"
        android:id="@+id/phone_title"
        android:padding="5dp"
        android:layout_below="@id/phone"/>

    <View style="@style/Divider"
        android:layout_marginBottom="5dp"
        android:layout_below="@id/phone_title"
        android:id="@+id/divider_contact3"/>
<!--....................................................................-->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Number not available"
        android:id="@+id/mobile"
        android:padding="5dp"
        android:layout_below="@id/divider_contact3"
        />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/call"
            android:layout_toLeftOf="@+id/add_to_contacts"
            android:background="@drawable/ic_action_call"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/divider_contact3"
            />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/add_to_contacts"
        android:layout_alignParentRight="true"
        android:background="@drawable/ic_action_add_person"
        android:layout_marginRight="15dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/divider_contact3"
    />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Mobile Number"
        android:id="@+id/mobile_title"
        android:padding="5dp"
        android:layout_below="@id/mobile"/>

    <View style="@style/Divider"
        android:layout_marginBottom="5dp"
        android:layout_below="@id/mobile_title"
        android:id="@+id/divider_contact4"/>

    <!--....................................................................-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Email not available"
        android:id="@+id/email"
        android:padding="5dp"
        android:layout_below="@id/divider_contact4"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Email"
        android:id="@+id/email_title"
        android:padding="5dp"
        android:layout_below="@id/email"/>

    <View style="@style/Divider"
        android:layout_marginBottom="5dp"
        android:layout_below="@id/email_title"
        android:id="@+id/divider_contact5"/>

    <!--....................................................................-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Blood Group not available"
        android:id="@+id/blood"
        android:padding="5dp"
        android:layout_below="@id/divider_contact5"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Blood Group"
        android:id="@+id/blood_title"
        android:padding="5dp"
        android:layout_below="@id/blood"/>

    <View style="@style/Divider"
        android:layout_marginBottom="5dp"
        android:layout_below="@id/blood_title"
        android:id="@+id/divider_contact6"/>

    <!--....................................................................-->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Club not available"
        android:id="@+id/club"
        android:padding="5dp"
        android:layout_below="@id/divider_contact6"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Club"
        android:id="@+id/club_title"
        android:padding="5dp"
        android:layout_below="@id/club"/>

    <!--<View style="@style/Divider"-->
        <!--android:layout_marginBottom="5dp"-->
        <!--android:layout_below="@id/club_title"-->
        <!--android:id="@+id/divider_contact7"/>-->

    <!--....................................................................-->

    <!--<TextView-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:textAppearance="?android:attr/textAppearanceMedium"-->
        <!--android:text="Club Address not available"-->
        <!--android:id="@+id/location"-->
        <!--android:padding="5dp"-->
        <!--android:layout_below="@id/divider_contact7"-->
        <!--/>-->

    <!--<TextView-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:textAppearance="?android:attr/textAppearanceSmall"-->
        <!--android:text="Club Location"-->
        <!--android:id="@+id/location_title"-->
        <!--android:padding="5dp"-->
        <!--android:layout_below="@id/location"/>-->


    <!--....................................................................-->

    </RelativeLayout>
    </ScrollView>
</LinearLayout>
